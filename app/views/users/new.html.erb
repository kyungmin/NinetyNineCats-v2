<h1>Sign up</h1>

<form action="<%= users_url %>" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">

  <label for="user_user_name">Username</label>
  <input type="text" name="user[user_name]" id="user_user_name"></input>
  <br />
  <label for="user_user_password">Password</label>
  <input type="password" name="user[password]" id="user_password"></input>
  <br />
  <input type="submit" value="Register">
</form>