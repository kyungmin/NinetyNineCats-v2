<br />
<br />
<table>
<tr>
  <th>Name</th>
  <th>Age</th>
  <th>Sex</th>
  <th>Color</th>
  <th>Birth date</th>
  <th>Owner</th>
</tr>
<% @cats.each do |cat| %>
  <tr>
    <td><%= link_to cat.name, cat_url(cat) %></td>
    <td><%= cat.age %></td>
    <td><%= cat.sex %></td>
    <td><%= cat.color %></td>
    <td><%= cat.birth_date %></td>
    <td><%= cat.owner.user_name %></td>
  </tr>
<% end %>
</table>